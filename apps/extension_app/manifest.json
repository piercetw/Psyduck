{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "Psyduck Extension",
    "action": {
        "default_popup": "js/index.html",
        "default_title": "Psyduck Extension"
    },
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "host_permissions": [
        "https://*/"
    ]
}